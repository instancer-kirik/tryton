<form on_write="on_written">
    <label name="name"/>
    <field name="name"/>
    <label name="version"/>
    <field name="version"/>
    <field name="dependencies" colspan="4"/>
    <label name="state"/>
    <field name="state" readonly="1"/>
    <group col="-1" colspan="2" id="button">
        <button name="upgrade"/>
        <button name="deactivate"/>
        <button name="activate"/>
        <button name="cancel"/>
    </group>
</form>
